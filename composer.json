        "monolog/monolog": "^2.6.0",
        "lucasmialichi/query-builder": "^1.1",
        "php-amqplib/php-amqplib": "^3.6",
        "lmcmi/validators": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
